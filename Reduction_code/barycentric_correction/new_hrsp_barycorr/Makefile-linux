EXE = apparent_position astronomical_almanac austral_flux \
      background_echelle compute_barycentric copy_keyword \
      correlate_images cosmic_rays create_thar \
      define_echelle dispersion_data dispersion_solution \
      extract_echelle extract_image filter_median \
      filter_star filter_white flat_extracted flat_full \
      form_eflat form_rflat form_white frame_image \
      hrsp_descriptors identify_object image_info \
      interpolate_solution list_descriptors list_keywords \
      modify_keywords normalize_spectrum pixel_shift \
      plot_echelle prepare_echelle radial_velocity \
      read_array read_regression rebin_echelle rebin_image \
      reduce_star reduce_white rotate_arbitrary show_fits \
      show_flux splice_echelle thorium_lines \
      transform_echelle quad_align unpack_echelle \
      velocity_correction vertical_offset

OBJ = angle.o astro.o config.o datetime.o fits.o general.o numeric.o \
      nutation.o process.o vector.o
HEAD = angle.h astro.h config.h datetime.h fits.h general.h numeric.h \
       nutation.h process.h vector.h

all: $(EXE)

angle.o: angle.c $(HEAD)
	gcc -Wall -c angle.c
astro.o: astro.c $(HEAD)
	gcc -Wall -c astro.c
config.o: config.c $(HEAD)
	gcc -Wall -c config.c
datetime.o: datetime.c $(HEAD)
	gcc -Wall -c datetime.c
fits.o: fits.c $(HEAD)
	gcc -Wall -c fits.c
general.o: general.c $(HEAD)
	gcc -Wall -c general.c
numeric.o: numeric.c $(HEAD)
	gcc -Wall -c numeric.c
nutation.o: nutation.c $(HEAD)
	gcc -Wall -c nutation.c
process.o: process.c $(HEAD)
	gcc -Wall -c process.c
vector.o: vector.c $(HEAD)
	gcc -Wall -c vector.c

apparent_position: apparent_position.o $(OBJ)
	gcc -Wall -o apparent_position $(OBJ) apparent_position.o -lm
apparent_position.o: apparent_position.c $(HEAD)
