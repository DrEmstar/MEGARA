EXE = apparent_position.exe astronomical_almanac.exe austral_flux.exe \
      background_echelle.exe compute_barycentric.exe copy_keyword.exe \
      correlate_images.exe cosmic_rays.exe create_thar.exe \
      define_echelle.exe dispersion_data.exe dispersion_solution.exe \
      extract_echelle.exe extract_image.exe filter_median.exe \
      filter_star.exe filter_white.exe flat_extracted.exe flat_full.exe \
      form_eflat.exe form_rflat.exe form_white.exe frame_image.exe \
      hrsp_descriptors.exe identify_object.exe image_info.exe \
      interpolate_solution.exe list_descriptors.exe list_keywords.exe \
      modify_keywords.exe normalize_spectrum.exe pixel_shift.exe \
      plot_echelle.exe prepare_echelle.exe radial_velocity.exe \
      read_array.exe read_regression.exe rebin_echelle.exe rebin_image.exe \
      reduce_star.exe reduce_white.exe rotate_arbitrary.exe show_fits.exe \
      show_flux.exe splice_echelle.exe thorium_lines.exe \
      transform_echelle.exe quad_align.exe unpack_echelle.exe \
      velocity_correction.exe vertical_offset.exe

OBJ = angle.o astro.o config.o datetime.o fits.o general.o numeric.o \
      nutation.o process.o vector.o
HEAD = angle.h astro.h config.h datetime.h fits.h general.h numeric.h \
       nutation.h process.h vector.h

all: $(EXE)

angle.o: angle.c $(HEAD)
	gcc -Wall -c angle.c
astro.o: astro.c $(HEAD)
	gcc -Wall -c astro.c
config.o: config.c $(HEAD)
	gcc -Wall -c config.c
datetime.o: datetime.c $(HEAD)
	gcc -Wall -c datetime.c
fits.o: fits.c $(HEAD)
	gcc -Wall -c fits.c
general.o: general.c $(HEAD)
	gcc -Wall -c general.c
numeric.o: numeric.c $(HEAD)
	gcc -Wall -c numeric.c
nutation.o: nutation.c $(HEAD)
	gcc -Wall -c nutation.c
process.o: process.c $(HEAD)
	gcc -Wall -c process.c
vector.o: vector.c $(HEAD)
	gcc -Wall -c vector.c

